{
  "name": "backend-blog-api",
  "version": "1.0.0",
  "engines": {
    "node": "10.21.0"
  },
  "description": "This is a backend API for my blog",
  "main": "index.js",
  "scripts": {
    "dev-mode": "nodemon --exec babel-node src/index.js",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/polyfill --require @babel/register --recursive --exit ./src/test/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "rm -rf ./build/ && babel ./src/ -d ./build/ && cp -r ./src/models/ ./build/models/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximeish/backend-blog-api.git"
  },
  "author": "Maxime Ishimwe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximeish/backend-blog-api/issues"
  },
  "homepage": "https://github.com/maximeish/backend-blog-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coverage": "^0.4.1",
    "coveralls": "^3.1.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "uniqid": "^5.2.0"
  }
}
